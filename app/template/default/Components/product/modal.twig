<div class="modal fade {{class|default('')}}" id="{{ id|default('eccube-modal') }}" tabindex="-1" role="dialog" aria-labelledby="ecModalLabel" aria-hidden="true">
    <div class="modal-dialog modal-dialog-centered" role="document">
        <div class="modal-content">
        <div class="modal-header bg-light">
            <h5 class="modal-title" id="ecModalLabel"></h5>
            <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                <span aria-hidden="true">&times;</span>
            </button>
        </div>
        <div class="modal-body text-center" id="modal-body"></div>
        <div class="modal-footer justify-content-center" id="modal-footer">
            <button id="close-modal" type="button" class="btn btn-secondary" data-dismiss="modal">
              {{ 'front.product.continue'|trans }}
            </button>
            <a id="go-cart" href="{{ url('cart') }}" class="btn btn-primary">{{ 'common.go_to_cart'|trans }}</a>
        </div>
        </div>
    </div>
</div>