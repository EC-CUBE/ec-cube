{% extends 'default_frame.twig' %}

{% set mypageno = 'change' %}

{% set body_class = 'mypage mypage_change_complete' %}

{% block main %}
    {% include "Components/heading.twig" with {'title': 'front.mypage.title', 'subTitle': 'front.mypage.nav__customer_complete' } %}

    {% include 'Mypage/navi.twig' %}
    
    <div class="row">
        <div class="col-12">
            <div class="p-4 text-center bg-light">
                <h4 class="pb-2">{{ 'front.mypage.customer_complete_message__title'|trans }}</h4>
                <p>{{ 'front.mypage.customer_complete_message__body'|trans }}</p>
                <hr class="my-4">
                <div class="col-md-3 px-0 mx-auto">
                    <a class="btn btn-primary btn-block" href="{{ url('homepage') }}">{{ 'common.back'|trans }}</a>
                </div>
            </div>
        </div>
    </div>
{% endblock %}
