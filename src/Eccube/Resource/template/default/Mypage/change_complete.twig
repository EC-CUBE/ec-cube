{% extends 'default_frame.twig' %}

{% set mypageno = 'change' %}

{% set body_class = 'mypage' %}

{% block main %}
    <div class="ec-layoutRole__main">
        <div class="ec-mypageRole">
            <div class="ec-pageHeader">
                <h1>{{'mypage.label.my_account'|trans}}/{{'mypage.label.customer_info_complete'|trans}}</h1>
            </div>
            {% include 'Mypage/navi.twig' %}
        </div>
        <div class="ec-registerCompleteRole">
            <div class="ec-off3Grid ec-text-ac">
                <div class="ec-off3Grid__cell">
                    <div class="ec-reportHeading">
                        <h2>会員登録内容の変更が完了いたしました</h2>
                    </div>
                    <p class="ec-reportDescription">{{'shopping.text.message.thank_you_with_us'|trans}}</p>
                    <div class="ec-off4Grid">
                        <div class="ec-off4Grid__cell"><a class="ec-blockBtn--cancel" href="{{ url('homepage') }}">{{'common.label.back_to_top'|trans}}</a></div>
                    </div>
                </div>
            </div>
        </div>
    </div>
{% endblock %}
