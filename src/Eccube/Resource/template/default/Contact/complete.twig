{% extends 'default_frame.twig' %}

{% block main %}
    <div class="ec-role">
        <div class="ec-pageHeader">
            <h1>お問い合わせ完了</h1>
        </div>
    </div>
    <div class="ec-contactCompleteRole">
        <div class="ec-off3Grid ec-text-ac">
            <div class="ec-off3Grid__cell">
                <div class="ec-reportHeading">
                    <h2>お問い合わせ内容の送信が完了いたしました</h2>
                </div>
                <p class="ec-reportDescription">{{'shopping.text.message.in_case'|trans}}</p>
                <div class="ec-off4Grid">
                    <div class="ec-off4Grid__cell"><a class="ec-blockBtn--cancel" href="{{ url('homepage') }}">{{'common.label.back_to_top'|trans}}</a></div>
                </div>
            </div>
        </div>
    </div>
{% endblock %}
