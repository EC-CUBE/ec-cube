{% extends 'default_frame.twig' %}

{% set menus = ['order', 'order_master'] %}

{% block title %}受注管理{% endblock %}
{% block sub_title %}メール通知{% endblock %}

{% block main %}
<div id="complete_wrap" class="row">
    <div id="complete_box" class="col-md-9">
        <div id="complete_box__body" class="box">
            <div id="complete_box__header" class="box-header">
                <h3 class="box-title">メール配信完了しました。</h3>
            </div><!-- /.box-header -->
        </div><!-- /.box -->
        <div id="complete_box__footer" class="row">
            <div id="complete_box__back_button" class="col-xs-10 col-xs-offset-1 col-sm-6 col-sm-offset-3 text-center btn_area">
                <p><a href="{{ url('admin_order_page', { page_no: app.session.get('eccube.admin.order.search.page_no')|default('1') }) }}?resume=1">検索結果へ戻る</a></p>
            </div>
        </div>

    </div><!-- /.col -->
</div>
{% endblock %}
