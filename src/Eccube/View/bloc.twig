{% if blocs|length > 0 %}
        {% for bloc in blocs %}
            <!-- ▼{{ bloc.blocName }} -->
            {% if bloc.phpPath %}
                {{ render(path('bloc_' ~ bloc.filename)) }}
            {% else %}
            	{{ include('frontparts/bloc/' ~ bloc.tplPath) }}
            {% endif %}
            <!-- ▲{{ bloc.blocName }} -->
        {% endfor %}
{% endif %}